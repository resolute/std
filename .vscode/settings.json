{
  "editor.tabSize": 2,
  "files.insertFinalNewline": true,
  "deno.enable": true,
  "deno.lint": true,
  "deno.unstable": true,
  "deno.config": "deno.json",
  "deno.importMap": "./import_map.json",
  "files.exclude": {
    "node_modules": true,
    "**/*.d.ts": true,
    "**/*.js": true,
    "**/*.js.map": true,
    "**/*.cjs": true,
    "**/*.mjs": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "cSpell.words": [
    "adamchal",
    "arrayify",
    "chainable",
    "codecov",
    "D’Xx",
    "dateify",
    "Deno",
    "denoland",
    "easings",
    "endregion",
    "httponly",
    "Mutators",
    "nonstring",
    "o'donnel",
    "O’Donnel",
    "O’Xx",
    "promisifying",
    "ucfirst"
  ],
  "deno.suggest.imports.hosts": {
    "https://deno.land": true
  }
}
